package intefaces;

import java.awt.*;
import javax.swing.*;


public class TesteJLabel extends JFrame {
	private JLabel label1, label2, label3;
	public TesteJLabel(){
		super("Teste de uso do JLabel");
		Container c = getContentPane();
		c.setLayout(new FlowLayout());
		
		label1 = new JLabel("Rotúlo do JLabel");
		ImageIcon icone1 = new javax.swing.ImageIcon(getClass.getResource("./figura.gif"));
		label2 = new JLabel ("Figura", icone, swingContants.LEFT);
		c.add(label1);
		c.add(label2);
		this.setPreferredSize(new Dimension(200,500));
		
	}
	
	
}
