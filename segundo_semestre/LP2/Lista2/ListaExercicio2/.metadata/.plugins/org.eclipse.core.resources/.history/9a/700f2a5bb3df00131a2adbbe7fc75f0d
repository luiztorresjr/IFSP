package br.soft.klon;

public class Variaveis {
	public Variaveis(){
		
	}
	float l;
	float R;
	float rMini;
	float beta1;
	float beta2;
	float beta3;
	float beta4;
	float pi=3.14159265359f;
	
	public float getL() {
		return l;
	}
	public void setL(float l) {
		this.l = l;
	}
	public float getR() {
		return R;
	}
	public void setR(float r) {
		R = r;
	}
	public float getrMini() {
		return rMini;
	}
	public void setrMini(float rMini) {
		this.rMini = rMini;
	}
	public float getBeta1() {
		return beta1;
	}
	public void setBeta1(float beta1) {
		this.beta1 = beta1;
	}
	public float getBeta2() {
		return beta2;
	}
	public void setBeta2(float beta2) {
		this.beta2 = beta2;
	}
	public float getBeta3() {
		return beta3;
	}
	public void setBeta3(float beta3) {
		this.beta3 = beta3;
	}
	public float getBeta4() {
		return beta4;
	}
	public void setBeta4(float beta4) {
		this.beta4 = beta4;
	}
	
	
	
}
